<h1> Edit Song </h1>

<% if flash[:errors] %>
  <ul>
    <% flash[:errors].each do |error| %>
      <li> <%= error%> </li>
    <% end %>
  </ul>
<%end%>

<%= form_for @song do |f| %>
  Title: <%= f.text_field :title %><br />
  Released : <<%= f.check_box :released %>><br />
  Release Year : <%= f.number_field :release_year %><br />
  Artist Name : <%= f.text_field :artist_name %><br />
  Genre : <%= f.text_field :genre %><br />

  <%= f.submit %>
<% end %>